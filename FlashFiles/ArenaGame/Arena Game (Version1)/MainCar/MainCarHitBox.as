package {	import flash.display.MovieClip;	import flash.events.Event;	public class MainCarHitBox extends MovieClip	{				public var mCar:MovieClip;		public var refM:MovieClip;			public function MainCarHitBox()		{			stage.addEventListener(Event.ENTER_FRAME,gLoop);						mCar = MovieClip(this.parent);			refM = MovieClip(mCar.parent);						refM.HitBoxesArray.push(this);		}		function gLoop(event:Event):void		{			this.alpha = 0;					}	}}