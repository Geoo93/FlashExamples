package {	import flash.display.MovieClip;	import flash.events.Event;	public class CarThreeHitBox extends MovieClip	{				public var cThree:MovieClip;		public var refM:MovieClip;			public function CarThreeHitBox()		{			stage.addEventListener(Event.ENTER_FRAME,gLoop);						cThree = MovieClip(this.parent);			refM = MovieClip(cThree.parent);						refM.HitBoxesArray.push(this);		}		function gLoop(event:Event):void		{			this.alpha = 0;					}	}}