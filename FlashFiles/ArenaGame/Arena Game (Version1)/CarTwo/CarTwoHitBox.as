package {	import flash.display.MovieClip;	import flash.events.Event;	public class CarTwoHitBox extends MovieClip	{				public var cTwo:MovieClip;		public var refM:MovieClip;			public function CarTwoHitBox()		{			stage.addEventListener(Event.ENTER_FRAME,gLoop);						cTwo = MovieClip(this.parent);			refM = MovieClip(cTwo.parent);						refM.HitBoxesArray.push(this);		}		function gLoop(event:Event):void		{			this.alpha = 0;					}	}}