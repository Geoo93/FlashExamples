package {	import flash.display.MovieClip;	import flash.events.Event;	public class CarFourHitBox extends MovieClip	{				public var cFour:MovieClip;		public var refM:MovieClip;			public function CarFourHitBox()		{			stage.addEventListener(Event.ENTER_FRAME,gLoop);						cFour = MovieClip(this.parent);			refM = MovieClip(cFour.parent);						refM.carFourBoxesArray.push(this);		}		function gLoop(event:Event):void		{			this.alpha = 0;					}	}}