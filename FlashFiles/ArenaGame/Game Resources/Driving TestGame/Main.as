package {	import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;	public class Main extends MovieClip	{		var gotoState:String = "mainCar";		var destinationX:int = 0;		var destinationY:int = 0;		var carsArray:Array = new Array();		var hit1Array:Array = new Array();		var mainCarArray:Array = new Array();		public function Main()		{			this.addEventListener(Event.ENTER_FRAME,gameLoop);			stage.addEventListener(MouseEvent.CLICK, mouseClick);		}		function gameLoop(myEvent:Event):void		{			for (var i:Number = 0; i < carsArray.length; i += 1)			{				if (gotoState == "mainCar")				{					destinationX = carsArray[0].x;					destinationY = carsArray[0].y;				}			}			for (var r:Number = 0; r < mainCarArray.length; r += 1)			{				mainCarArray[r].alpha = 0.1				;				for (var q:Number = 0; q < hit1Array.length; q += 1)				{					hit1Array[q].alpha = 0.1;					if (hit1Array[q].hitTestObject(mainCarArray[r]))					{						carsArray[1].alpha = .5;						//car11.issActive = false;						//car11.isTrue22 = true;						//car11.startMove2 = false;					}				}			}		}		public function mouseClick(event: MouseEvent):void		{		}	}}