package {	import flash.display.MovieClip;	import flash.events.Event;	public class Car5 extends MovieClip	{		var refM:MovieClip;		public function Car5()		{			this.addEventListener(Event.ENTER_FRAME,gLoop);			refM = MovieClip(this.parent);		}		function gLoop(event:Event):void		{			this.y -=  0.4;						if (this.y < 555)			{				this.y +=  0.4;			}						if (this.car5hitbox.hitTestObject(refM.motobike.hitboxes2a))			{				refM.playagainbut.hitState = true;				refM.motobike.motobikeState = false;			}			if (this.car5hitbox.hitTestObject(refM.motobike.hitboxes2b))			{				refM.playagainbut.hitState = true;				refM.motobike.motobikeState = false;			}					}	}}