package  {		import flash.display.Sprite;			public class Main extends Sprite {				public function Main() {			addChild(new AnimatedRamp(stage.stageWidth, stage.stageHeight));			// add a pulser and add a controller to the pulser			var pulse:Pulser = new Pulser();			pulse.x = stage.stageWidth/2;			pulse.y = stage.stageHeight/2;			pulse.addChild(new Controller());			addChild(pulse);		}	}	}